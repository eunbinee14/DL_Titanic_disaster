   "source": [
    "bins = [ 0, 4, 12, 18, 30, 50, 65, 100] # 임시 지정 \n",
    "age_index = (1,2,3,4,5,6,7) #('baby','child','teenager','young','mid-age','over-50','senior')\n",
    "df_train['Age-bin'] = pd.cut(df_train.Age, bins, labels=age_index).astype(int)\n",
    "\n",
    "df_train[['Age-bin', 'Survived']].groupby(['Age-bin'],as_index=False).mean()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "778867db",
